{
  "name": "@svelteaction/integrations",
  "version": "3.0.12",
  "description": "Integration wrappers for utility libraries",
  "author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
  "license": "MIT",
  "homepage": "https://github.com/Mohamed-Kaizen/svelteaction/tree/main/packages/integrations#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mohamed-Kaizen/svelteaction.git",
    "directory": "packages/integrations"
  },
  "bugs": {
    "url": "https://github.com/Mohamed-Kaizen/svelteaction/issues"
  },
  "keywords": [
    "svelte",
    "svelte-action",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./*": "./*",
    "./useJwt": {
      "types": "./useJwt.d.ts",
      "import": "./useJwt.mjs",
      "require": "./useJwt.cjs"
    },
    "./useChangeCase": {
      "types": "./useChangeCase.d.ts",
      "import": "./useChangeCase.mjs",
      "require": "./useChangeCase.cjs"
    },
    "./useQRCode": {
      "types": "./useQRCode.d.ts",
      "import": "./useQRCode.mjs",
      "require": "./useQRCode.cjs"
    }
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "unpkg": "./index.iife.min.js",
  "jsdelivr": "./index.iife.min.js",
  "types": "./index.d.ts",
  "peerDependencies": {
    "change-case": "*",
    "jwt-decode": "*",
    "qrcode": "*"
  },
  "peerDependenciesMeta": {
    "change-case": {
      "optional": true
    },
    "jwt-decode": {
      "optional": true
    },
    "qrcode": {
      "optional": true
    }
  },
  "dependencies": {
    "@svelteaction/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.0",
    "change-case": "^4.1.2",
    "jwt-decode": "^3.1.2",
    "qrcode": "^1.5.1"
  }
}
