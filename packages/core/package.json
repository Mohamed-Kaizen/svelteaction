{
  "name": "@svelteaction/core",
  "version": "1.1.0",
  "description": "Collection of essential Svelte Utilities",
  "author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
  "license": "MIT",
  "homepage": "https://github.com/Mohamed-Kaizen/svelteaction#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mohamed-Kaizen/svelteaction.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/Mohamed-Kaizen/svelteaction/issues"
  },
  "keywords": [
