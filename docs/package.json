		"@iconify-json/ri": "^1.1.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@svelteness/kit-docs": "^0.22.12",
		"@types/react": "^18.0.14",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"clsx": "^1.1.1",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"shiki": "^0.10.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2",
		"unplugin-icons": "^0.13.4"
	},
	"dependencies": {
		"@svelteaction/core": "workspace:*",
		"@svelteaction/shared": "workspace:*",
		"@svelteaction/integrations": "workspace:*"
